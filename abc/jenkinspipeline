pipeline {
    agent any

    stages {
        stage('Printing Hello world on jenkins') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Printing Hi on jenkins') {
            steps {
                echo 'HI'
            }
        }
        stage('STAGE3') {
            steps {
                echo 'HI'
            }
        }
        stage('STAGE4') {
            steps {
                echo 'HI'
            }
        }
    }
}
