pipeline {
    agent any

    stages {
        stage('colour of the book') {
            steps {
                echo 'black'
            }
        }
        stage('colour of the rainbow') {
            steps {
                echo 'vibgyor'
            }
        }
        stage('colour of rice') {
            steps {
                echo 'white'
            }
        }
        stage('colour of sun') {
            steps {
                echo 'Orange'
            }
        }
    }
}
